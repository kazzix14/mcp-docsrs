{
  "mcpServers": {
    "context7": {
      "type": "sse",
      "url": "https://mcp.context7.com/sse"
    },
    "mcp-deepwiki": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "mcp-deepwiki@latest"
      ],
      "env": {}
    },
    "mcp-interactive": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "interactive-mcp@1.9.0"
      ],
      "env": {}
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    },
    "mcp-docsrs": {
      "type": "stdio",
      "command": "./dist/mcp-docsrs",
      "args": [
        "--cache-ttl=7200000",
        "--max-cache-size=200",
        "--db-path=./.cache"
      ]
    }
  }
}